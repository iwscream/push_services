<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>./WEB-INF/index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>SpeakSubmit</servlet-name>
        <servlet-class>speak.SpeakSubmit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SpeakSubmit</servlet-name>
        <url-pattern>/speak/SpeakSubmit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckOutPassword</servlet-name>
        <servlet-class>speak.CheckOutPassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckOutPassword</servlet-name>
        <url-pattern>/speak/CheckOutPassword</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BrowseList</servlet-name>
        <servlet-class>browse.BrowseList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BrowseList</servlet-name>
        <url-pattern>/browse/BrowseList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BrowseContent</servlet-name>
        <servlet-class>browse.BrowseContent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BrowseContent</servlet-name>
        <url-pattern>/browse/BrowseContent</url-pattern>
    </servlet-mapping>

